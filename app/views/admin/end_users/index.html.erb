<div class="container">
  <div class="row mt-5">
    <h4>ユーザー一覧</h4>
    </div>
    <div class="row mt-3">
    <div class="col-8 offset-2">
      <table class="table">
      <thead>
        <tr>
          <th>ニックネーム</th>
          <th>ステータス</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <% @end_users.each do |end_user| %>
          <td><%= link_to admin_end_user_path(end_user.id) do %><%= end_user.nickname %><% end %></td>
          <td>
            <% if end_user.is_deleted == false %>
              <div class="text-success">有効</div>
            <% else %>
              <div class="text-secondary">退会</div>
            <% end %>
          </td>
        </tr>
        <% end %>
      </tbody>
    </table>
    </div>
  </div>
</div>
